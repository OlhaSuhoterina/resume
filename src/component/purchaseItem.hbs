<div class="col-12">
    {{#each itemList}}
    <div class="row">
        <div class="col-4">
            <p>{{number}}</p>
            <b>{{status}}</b>
        </div>
        <div class="col-2">
            {{amount.title}}: <br />
            <b>{{amount.value}}</b>
        </div>
        <div class="col-4">
            {{#each images}}
            <img src="{{this}}" alt="photo" style=" border-radius:10px; width: 60px; height:60px">
            {{/each}}
        </div>
        <div class="col-2 ">
            {{#with button}}
            {{#if isPrimary}}<button type="button" class="btn btn-primary ">{{text}}</button>{{/if}}
            {{/with}}
        </div>
        <hr>
    </div>
    {{/each}}
</div>